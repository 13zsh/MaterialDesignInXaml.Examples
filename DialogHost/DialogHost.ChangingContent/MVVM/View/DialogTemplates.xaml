<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:DialogHost.ChangingContent.MVVM.ViewModel"
                    xmlns:local="clr-namespace:DialogHost.ChangingContent.MVVM.View">

    <DataTemplate DataType="{x:Type vm:LoginViewModel}">
        <local:LoginControl />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CreateAccountViewModel}">
        <local:CreateAccountControl />
    </DataTemplate>
</ResourceDictionary>